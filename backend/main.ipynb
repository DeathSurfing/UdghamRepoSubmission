    "import sys\n",
    "\n",
    "# Check if two arguments are provided\n",
    "if len(sys.argv) != 3:\n",
    "    print(\"Usage: python script.py <arg1> <arg2>\")\n",
    "    sys.exit(1)\n",
    "\n",
    "# Get arguments\n",
    "arg1 = sys.argv[1]\n",
    "arg2 = sys.argv[2]\n",
    "\n",
    "print(f\"Argument 1: {arg1}\")\n",
    "print(f\"Argument 2: {arg2}\")\n"
   ]
  }
